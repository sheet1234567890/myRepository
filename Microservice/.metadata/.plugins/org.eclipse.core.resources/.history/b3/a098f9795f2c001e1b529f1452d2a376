spring.kafka.consumer.bootstrap-servers=localhost:9092
spring.kafka.consumer.group-id=mygroup
spring.kafka.consumer.auto-offset-reset=earliest
spring.kafka.consumer.key-deserializer=org.apache.kafka.comman.serialization-stringDeserializer
spring.kafka.consumer.value-deserializer=org.apache.kafka.comman.serialization-stringDeserializer


spring.kafka.producer.bootstrap-servers=localhost:9092
spring.kafka.producer.key-serializer=org.apache.kafka.comman.serialization.StringSerializer
spring.kafka.producer.value-serializer=org.apache.kafka.comman.serialization.StringSerializer